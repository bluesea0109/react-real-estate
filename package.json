{
  "name": "beta-brivitymarketer-com",
  "homepage": "/v2/",
  "version": "2.2.13",
  "private": true,
  "scripts": {
    "start": "PORT=8082 react-scripts start",
    "build": "(export REACT_APP_SENTRY_RELEASE=$(node -pe \"require('./package.json').version\"); react-scripts build && node utils/sentry.js)",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public tmp artifacts .",
    "format": "prettier --write 'src/**/*.{js,jsx,json}'",
    "generate-artifacts": "node utils/generate-testing-artifacts",
    "create": "node utils/create-campaigns",
    "reset": "node utils/resetDevDb",
    "commit": "node utils/commit",
    "push-to-production": "git checkout production && git merge master && git push origin production && git checkout master"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.26",
    "@fortawesome/free-solid-svg-icons": "5.12.0",
    "@fortawesome/react-fontawesome": "0.1.8",
    "@sentry/browser": "5.11.1",
    "auth0-js": "9.12.2",
    "date-fns": "2.9.0",
    "dinero.js": "1.6.0",
    "formik": "^2.1.3",
    "google-maps-react": "2.0.2",
    "js-cookie": "2.2.1",
    "lodash": "4.17.15",
    "nouislider": "14.1.1",
    "nouislider-react": "3.3.5",
    "prop-types": "15.7.2",
    "pure-react-carousel": "1.17.0",
    "react": "^16.12.0",
    "react-color": "2.18.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "10.2.1",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-router-last-location": "2.0.1",
    "react-scripts": "3.3.0",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "0.88.2",
    "styled-components": "4.4.1",
    "yup": "0.28.1"
  },
  "devDependencies": {
    "@sentry/cli": "1.49.0",
    "http-proxy-middleware": "0.20.0",
    "husky": "4.2.1",
    "lint-staged": "10.0.3",
    "prettier": "1.19.1",
    "react-test-renderer": "16.12.0",
    "request": "2.88.0",
    "request-promise": "4.2.5"
  },
  "optionalDependencies": {
    "alf-commons": "git+ssh://git@gitlab.rmcloud.com/gabby/alf-common.git",
    "joi-to-json-schema": "5.1.0",
    "json-schema-faker": "0.5.0-rc23"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "npm run format",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(dinero.js)/)"
    ]
  }
}
