{
  "name": "beta-brivitymarketer-com",
  "homepage": "/v2/",
  "version": "5.0.1",
  "private": true,
  "scripts": {
    "start": "cross-env PORT=8082 craco start",
    "build": "(export REACT_APP_SENTRY_RELEASE=$(node -pe \"require('./package.json').version\"); craco build && node utils/sentry.js)",
    "build-local": "craco build",
    "test": "npm run formatCheck",
    "eject": "craco eject",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public tmp artifacts .",
    "format": "prettier --write 'src/**/*.{js,jsx,json}'",
    "formatCheck": "prettier --check 'src/**/*.{js,jsx,json}'",
    "generate-artifacts": "node utils/generate-testing-artifacts",
    "create": "node utils/create-campaigns",
    "reset": "node utils/resetDevDb",
    "commit": "node utils/commit",
    "push-to-production": "git checkout production && git merge master && git push origin production && git checkout master",
    "postinstall": "node utils/postinstall"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.26",
    "@fortawesome/free-solid-svg-icons": "5.12.0",
    "@fortawesome/react-fontawesome": "0.1.8",
    "@react-google-maps/api": "^1.9.0",
    "@sentry/browser": "5.11.1",
    "auth0-js": "9.12.2",
    "date-fns": "2.9.0",
    "dinero.js": "1.6.0",
    "formik": "^2.1.3",
    "google-maps-react": "2.0.2",
    "js-cookie": "2.2.1",
    "lodash": "^4.17.20",
    "node-sass": "^4.14.1",
    "nouislider": "14.1.1",
    "nouislider-react": "3.3.5",
    "prop-types": "15.7.2",
    "pure-react-carousel": "1.17.0",
    "react": "^16.12.0",
    "react-color": "^2.18.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "10.2.1",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-router-last-location": "2.0.1",
    "react-scripts": "^3.4.3",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3",
    "semantic-ui-react": "0.88.2",
    "styled-components": "4.4.1",
    "yup": "0.28.1"
  },
  "devDependencies": {
    "@craco/craco": "^5.8.0",
    "@semantic-ui-react/craco-less": "^1.2.1",
    "@sentry/cli": "1.49.0",
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "http-proxy-middleware": "0.20.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.12.0",
    "request": "2.88.0",
    "request-promise": "4.2.5",
    "semantic-ui-less": "^2.4.1"
  },
  "optionalDependencies": {
    "joi-to-json-schema": "5.1.0",
    "json-schema-faker": "0.5.0-rc23"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(dinero.js)/)"
    ]
  }
}
